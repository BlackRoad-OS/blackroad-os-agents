{
  "name": "blackroad-os-agents",
  "version": "0.2.0",
  "private": true,
  "description": "Central registry, SDK, and platform integrations for Blackroad OS agents",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "npm run test:js && npm run test:py",
    "test:js": "NODE_OPTIONS=--experimental-vm-modules vitest run",
    "test:py": "pytest",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "br-agent": "ts-node --esm src/cli.ts",
    "postbuild": "ts-node --esm scripts/postbuild.ts",
    "validate": "node scripts/validate_agents.ts",
    "start": "node dist/server.js",
    "dev": "ts-node --esm src/server.ts"
  },
  "files": [
    "dist",
    "registry",
    "public/sig.beacon.json"
  ],
  "bin": {
    "br-agent": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./integrations": {
      "types": "./dist/integrations/index.d.ts",
      "default": "./dist/integrations/index.js"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.0",
    "@huggingface/hub": "^0.15.0",
    "@huggingface/inference": "^2.8.0",
    "@notionhq/client": "^2.2.0",
    "asana": "^3.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "got": "^14.0.0",
    "octokit": "^4.0.0",
    "stripe": "^14.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
